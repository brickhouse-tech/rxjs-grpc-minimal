{
  "name": "rxjs-grpc-minimal",
  "version": "0.2.8",
  "description": "grpc node callback and streams wrapped in observables",
  "main": "src/index.js",
  "license": "MIT",
  "keywords": [
    "rxjs",
    "grpc",
    "protobuf",
    "protobuffers",
    "reactive",
    "proto"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/brickhouse-tech/rxjs-grpc-minimal.git"
  },
  "files": [
    "src",
    "*.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "server": "node ./examples/helloworld/server.js",
    "client": "node ./examples/helloworld/client.js",
    "client2": "node ./examples/helloworld/client2.js"
  },
  "dependencies": {
    "debug-fabulous": "^2.0.1",
    "rxjs": "^7.8.1",
    "through2": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@grpc/grpc-js": "^1.12.5",
    "@grpc/proto-loader": "^0.8.0",
    "@vitest/coverage-v8": "^4.0.18",
    "commit-and-tag-version": "^12.6.1",
    "eslint": "^9.39.2",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-promise": "^7.2.1",
    "lodash": "^4.17.21",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18"
  }
}
